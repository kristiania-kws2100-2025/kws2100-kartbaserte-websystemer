  },
  "homepage": "https://github.com/kristiania-kws2100-2025/kws2100-kartbaserte-websystemer#readme",
  "description": "",
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "husky": "^9.1.7",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3",
    "vite": "^6.2.0"
  },
  "dependencies": {
    "download-cli": "^1.1.1",
    "ol": "^10.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
