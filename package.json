  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/kristiania-kws2100-2025/kws2100-kartbaserte-websystemer/issues"
  },
  "homepage": "https://github.com/kristiania-kws2100-2025/kws2100-kartbaserte-websystemer#readme",
  "devDependencies": {
    "@types/node": "^22.13.16",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.4"
  },
  "dependencies": {
    "download-cli": "^1.1.1",
    "ol": "^10.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
